package mhoctypes

import (
	"encoding/hex"
	"fmt"
	pdubuilder "github.com/onosproject/onos-e2-sm/servicemodels/e2sm_mho/pdubuilder"
	e2sm_mho "github.com/onosproject/onos-e2-sm/servicemodels/e2sm_mho/v1/e2sm-mho" //ToDo - Make imports more dynamic
	"gotest.tools/assert"
	"testing"
)

func createNrcellIDentityMsg() (*e2sm_mho.NrcellIdentity, error) {

	// nrcellIDentity := pdubuilder.CreateNrcellIDentity() //ToDo - fill in arguments here(if this function exists

	nrcellIDentity := e2sm_mho.NrcellIdentity{
		Value: nil,
	}

	if err := nrcellIDentity.Validate(); err != nil {
		return nil, fmt.Errorf("error validating NrcellIdentity %s", err.Error())
	}
	return &nrcellIDentity, nil
}

func Test_xerEncodingNrcellIDentity(t *testing.T) {

	nrcellIDentity, err := createNrcellIDentityMsg()
	assert.NilError(t, err, "Error creating NrcellIdentity PDU")

	xer, err := xerEncodeNrcellIDentity(nrcellIDentity)
	assert.NilError(t, err)
	assert.Equal(t, 1, len(xer)) //ToDo - adjust length of the XER encoded message
	t.Logf("NrcellIDentity XER\n%s", string(xer))

	result, err := xerDecodeNrcellIDentity(xer)
	assert.NilError(t, err)
	assert.Assert(t, result != nil)
	t.Logf("NrcellIDentity XER - decoded\n%v", result)
	//ToDo - adjust field's verification
	assert.Equal(t, nrcellIDentity.GetValue(), result.GetValue())

}

func Test_perEncodingNrcellIDentity(t *testing.T) {

	nrcellIDentity, err := createNrcellIDentityMsg()
	assert.NilError(t, err, "Error creating NrcellIdentity PDU")

	per, err := perEncodeNrcellIDentity(nrcellIDentity)
	assert.NilError(t, err)
	assert.Equal(t, 1, len(per)) // ToDo - adjust length of the PER encoded message
	t.Logf("NrcellIDentity PER\n%v", hex.Dump(per))

	result, err := perDecodeNrcellIDentity(per)
	assert.NilError(t, err)
	assert.Assert(t, result != nil)
	t.Logf("NrcellIDentity PER - decoded\n%v", result)
	//ToDo - adjust field's verification
	assert.Equal(t, nrcellIDentity.GetValue(), result.GetValue())

}
